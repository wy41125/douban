.itme-navigator{
  width: 210rpx;
  margin-right: 20rpx;
  display: inline-block;
}
.itme-navigator .item-group {
  width: 100%;
}
.item-group .thumbnail-group{
  width: 100%;
  height: 284rpx;
}
.thumbnail-group .thumbnail{
  width: 100%;
  height: 100%; 
}
.item-group .item-title{
  font-size: 28rpx;
  text-align: center;
  margin-top: 10rpx;
  overflow:hidden; /*内容超出宽度时隐藏超出部分的内容 */
  white-space:nowrap; /*不换行 */
  text-overflow:ellipsis;
  margin-bottom: 20rpx;
}